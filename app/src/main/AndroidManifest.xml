<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.apps.franco.musicplayerapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/guitarra"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ListOfArtists"
            android:label="List of Artist"/>
        <activity android:name=".Trivium"
            android:label="Trivium"/>
        <activity android:name=".Rhapsody"
            android:label="Rhapsody"/>
        <activity android:name=".Killswitch"
            android:label="Killswitch"/>
        <activity android:name=".Purchase"
            android:label="PURCHASE ACTIVITY"/>

    </application>

</manifest>